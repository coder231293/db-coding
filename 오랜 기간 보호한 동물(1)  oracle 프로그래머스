SELECT INS.NAME, INS.DATETIME 
FROM ANIMAL_INS INS LEFT OUTER JOIN ANIMAL_OUTS OUTS ON INS.ANIMAL_ID = OUTS.ANIMAL_ID 
WHERE OUTS.DATETIME IS NULL AND WHERE INS.ROWNUM = 3 
ORDER BY INS.DATETIME

SELECT NAME, DATETIME 
FROM (SELECT * FROM ANIMAL_INS ORDER BY DATETIME) 
WHERE ANIMAL_ID NOT IN(SELECT ANIMAL_ID FROM ANIMAL_OUTS) AND ROWNUM <= 3 
